{
  "name": "notify-eu/notify-bundle",
  "description": "Notify bundle for Symfony",
  "keywords": [
    "notify",
    "aranealabs",
    "notification",
    "notifications",
    "bundle",
    "symfony"
  ],
  "homepage": "https://github.com/notify-eu/notify-bundle",
  "type" : "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Notify",
      "email": "info@notify.eu",
      "homepage": "https://notify.eu"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "^4.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "symfony/phpunit-bridge": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "NotifyEu\\NotifyBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NotifyEu\\NotifyBundle\\Tests\\": "tests/"
    }
  }
}